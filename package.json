{
  "name": "soundboxing-client",
  "version": "0.1.7",
  "description": "Client library for accessing the Soundboxing API.",
  "main": "main.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "protobuf": "rm -f lib/v1/serialization.js && pbjs -t static-module -o lib/v1/serialization.js lib/v1/soundboxing.proto",
    "prettier": "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --write \"lib/**/client.js\" \"*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericflo/soundboxing-client.git"
  },
  "keywords": [
    "soundboxing",
    "vr",
    "virtual",
    "reality",
    "client",
    "library",
    "music",
    "rhythm"
  ],
  "author": "Eric Florenzano <floguy@gmail.com> (http://www.eflorenzano.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericflo/soundboxing-client/issues"
  },
  "homepage": "https://github.com/ericflo/soundboxing-client#readme",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^1.0.0",
    "protobufjs": "^6.8.6",
    "query-string": "^6.0.0",
    "string-to-stream": "^1.1.0",
    "uglifyjs-webpack-plugin": "^1.2.4"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
